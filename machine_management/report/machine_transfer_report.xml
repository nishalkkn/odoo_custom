<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <template id="machine_transfer_report">
        <t t-call="web.html_container">
            <t t-foreach="docs" t-as="i">
                <t t-call="web.external_layout">
                    <div class="page">
                        <div class="oe_structure"/>
                        <h2>Machine Transfer Report</h2>
                        <br></br>
                        <p>
                            Machine :
                            <span t-field="i.machine_id"/><br/>
                            Transfer date :
                            <span t-field="i.transfer_date"/><br/>
                            Transfer type :
                            <span t-field="i.transfer_type"/><br/>
                            Customer :
                            <span t-field="i.customer_id"/><br/>
                        </p>
                    </div>
                </t>
            </t>
        </t>
    </template>
</odoo>
